{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/insurance",
      "dest": "/industries/insurance/pitch.html"
    },
    {
      "src": "/insurance/demo",
      "dest": "/industries/insurance/demo.html"
    },
    {
      "src": "/construction",
      "dest": "/industries/construction/pitch.html"
    },
    {
      "src": "/construction/demo",
      "dest": "/industries/construction/demo.html"
    },
    {
      "src": "/healthcare",
      "dest": "/industries/healthcare/coming-soon.html"
    },
    {
      "src": "/case-studies",
      "dest": "/case-studies/success-stories.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
