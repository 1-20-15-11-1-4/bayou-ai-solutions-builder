{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/insurance",
      "dest": "/industries/insurance/pitch.html"
    },
    {
      "src": "/construction", 
      "dest": "/industries/construction/pitch.html"
    },
    {
      "src": "/healthcare",
      "dest": "/industries/healthcare/coming-soon.html"
    }
  ]
}
